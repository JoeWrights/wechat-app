.button{
    display: inline-block;
    margin: 10rpx;
    background: #e0e1e2;
    padding: 0 40rpx;
    color: rgba(0,0,0,.6);
    font-weight: bold;
    border-radius: 1rpx;
    font-size: 32rpx;
    transition: 0.3s all;
}
.button.primary{
    background: #000;
    color: rgba(225,225,225,.9);
}